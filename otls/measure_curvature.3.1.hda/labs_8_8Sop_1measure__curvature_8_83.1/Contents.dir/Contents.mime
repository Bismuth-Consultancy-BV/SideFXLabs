--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/pointwrangle4.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	!_mcu_unshared	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"// compute largest eigenvector of 3x3 matrix
vector eigenvector(const matrix3 mat_input; const int max_iter; const float tol)
{
        matrix3 mat = mat_input;
        vector vec = 1;
        vector vec_max = normalize(vec * mat);
        vector vec_max_temp;
        
        int i = max_iter;
        while (i--)
        {
            vec_max_temp = vec_max;
            vec_max *= mat;
            vec_max = normalize(vec_max);
            int cond = distance(pow(vec_max, 2), pow(vec_max_temp, 2)) < tol;
            if (cond)
                break;
        }
        return vec_max;
}

int neighbours[] = neighbours(0, @ptnum);
@N = normalize(@N);

float mat_array[];
float area_sum = 0;

foreach (int neighbour; neighbours)
{
    vector pos = point(0, \"P\", neighbour);
    vector edge = pos - @P;
    matrix3 mat_p = ident() - outerproduct(@N, @N);
    vector edge_p = normalize(edge * mat_p);
    
    // compute directional curvature along edge
    float curv_d = 2 * f@_mcu_bound_size * dot(edge, @N) / dot(edge, edge);
   
    // compute weights
    float area = 0;
    int prim1 = hedge_prim(0, pointhedge(0, @ptnum, neighbour));
    int prim2 = hedge_prim(0, pointhedge(0, neighbour, @ptnum));
    if (prim1 != -1)
        area += primintrinsic(0, \"measuredarea\", prim1);
    if (prim2 != -1)
        area += primintrinsic(0, \"measuredarea\", prim2);
    area_sum += area;
    
    // build shape operator
    for (int i = 0; i < 9; i++)
        mat_array[i] += area * curv_d * edge_p[i / 3] * edge_p[i % 3];
}

// normalize by area
for (int i = 0; i < 9; i++)
    mat_array[i] /= area_sum;
    
// get eigenvectors of matrix
matrix3 mat = set(mat_array); 
vector eigvec2 = normalize(eigenvector(mat, 100, 1e-5));
vector eigvec1 = normalize(cross(@N, eigvec2));

// compute corresponding eigenvalues
float m11 = (matrix3(set(eigvec1 * mat, 0, 0)) * transpose(matrix3(set(eigvec1, 0, 0)))).xx;
float m22 = (matrix3(set(eigvec2 * mat, 0, 0)) * transpose(matrix3(set(eigvec2, 0, 0)))).xx;   

// compute curvatures
float k1 = 3 * m11 - m22;
float k2 = 3 * m22 - m11;
float mean_curv = (k1 + k2) * 0.5;
float gauss_curv = k1 * k2;
vector min_curv_dir = normalize(eigvec1);
vector max_curv_dir = normalize(eigvec2);

// set attributes
f@_mcu_mean_curv = mean_curv;
f@_mcu_gauss_curv = gauss_curv;
f@_mcu_min_curv = k1;
f@_mcu_max_curv = k2;
v@_mcu_min_curv_dir = min_curv_dir;
v@_mcu_max_curv_dir = max_curv_dir;
f@_mcu_curvature = f@_mcu_mean_curv;"	)

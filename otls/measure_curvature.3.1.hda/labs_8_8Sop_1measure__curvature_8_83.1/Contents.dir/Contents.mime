snippet	[ 0	locks=0 ]	(	"// compute largest eigenvector of 3x3 matrix
vector eigenvector(const matrix3 mat_input; const int max_iter; const float tol)
{
        matrix3 mat = mat_input;
        vector vec = 1;
        vector vec_max = normalize(vec * mat);
        vector vec_max_temp;
        
        int i = max_iter;
        while (i--)
        {
            vec_max_temp = vec_max;
            vec_max *= mat;
            vec_max = normalize(vec_max);
            int cond = distance(pow(vec_max, 2), pow(vec_max_temp, 2)) < tol;
            if (cond)
                break;
        }
        return vec_max;
}

// set matrix
matrix3 mat = set(v@_mcu_dx, v@_mcu_dy, v@_mcu_dz);

float norm = 0;
for (int i = 0; i < 9; i++)
    norm += pow(getcomp(mat, i / 3, i % 3), 2);

// compute curvatures
float mean_curv = (mat.xx + mat.yy + mat.zz) * -0.5;
float gauss_curv = pow(mean_curv, 2) * 2 - norm / 2;    
float k1 = mean_curv - sqrt(norm / 2 - pow(mean_curv, 2));
float k2 = mean_curv + sqrt(norm / 2 - pow(mean_curv, 2));


// compute principal curvature directions
vector max_curv_dir = normalize(eigenvector(mat, 100, 1e-5));
vector min_curv_dir = normalize(cross(@N, max_curv_dir));

// set attributes
f@_mcu_mean_curv = mean_curv;
f@_mcu_gauss_curv = gauss_curv;
f@_mcu_min_curv = k1;
f@_mcu_max_curv = k2;
v@_mcu_min_curv_dir = min_curv_dir;
v@_mcu_max_curv_dir = max_curv_dir;

f@_mcu_curvature = mean_curv;"	)

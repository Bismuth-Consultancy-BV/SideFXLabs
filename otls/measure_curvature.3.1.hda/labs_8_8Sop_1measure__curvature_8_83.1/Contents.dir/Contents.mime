--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/pointwrangle6.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	!_mcu_unshared	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int neighbours[] = neighbours(0, @ptnum);
int num = len(neighbours);

// initializes arrays
vector edge_array[];
int neighbour_array[];
float curv_array[];

foreach (int i; int neighbour; neighbours)
{
    vector pos = point(0, \"P\", neighbour);
    vector normal = point(0, \"N\", neighbour);
    vector edge = pos - @P;
    
    // project edge onto tangent plane at current point
    matrix3 mat_p = ident() - outerproduct(@N, @N);
    vector edge_p = normalize(edge * mat_p);
    
    // compute normal curvature along the edge
    float curv_d = dot(edge, normal - @N) * -1 / dot(edge, edge);
    
    edge_array[i] = edge_p;
    neighbour_array[i] = neighbour;
    curv_array[i] = curv_d;
}

// sort arrays based on normal curvatures
int index_array[] = argsort(curv_array);
neighbour_array = reorder(neighbour_array, index_array);
edge_array = reorder(edge_array, index_array);
curv_array = reorder(curv_array, index_array);

// compute basis vectors on tangent plane
vector e1 = edge_array[-1];
vector e2 = normalize(cross(e1, normalize(@N)));

// use euler formula and solve for b and c by using least squares
float a = curv_array[-1];
float a11 = 0, a12 = 0, a21 = 0, a22 = 0, a13 = 0, a23 = 0;
for (int i = 0; i < num; i++)
{
    float angle = acos(dot(edge_array[i], e1));
    a11 += pow(cos(angle), 2) * pow(sin(angle), 2);
    a12 += cos(angle) * pow(sin(angle), 3);
    a22 += pow(sin(angle), 4);
    a13 += (curv_array[i] - a * pow(cos(angle), 2)) * cos(angle) * sin(angle);
    a23 += (curv_array[i] - a * pow(cos(angle), 2)) * pow(sin(angle), 2);
}
a21 = a12;
float b = (a13 * a22 - a23 * a12) / (a11 * a22 - a12 * a12);
float c = (a11 * a23 - a12 * a13) / (a11 * a22 - a12 * a12);


// calculate curvature
float gaussian_curv = a * c - pow(b, 2) / 4;
float mean_curv = (a + c) / 2;

// calculate principal curvatures
float k1 = mean_curv - sqrt(mean_curv * mean_curv - gaussian_curv);
float k2 = mean_curv + sqrt(mean_curv * mean_curv - gaussian_curv);

// calculate principal curvature directions
float angle = 1;
if (k1 != k2)
    angle = 0.5 * asin(b / (k2 - k1));
    
vector vec1 = cos(angle) * e1 + sin(angle) * e2;
vector vec2 = cos(angle) * e2 - sin(angle) * e1;

// set attributes
f@_mcu_mean_curv = mean_curv;
f@_mcu_gauss_curv = gaussian_curv;
f@_mcu_min_curv = k1;
f@_mcu_max_curv = k2;
v@_mcu_min_curv_dir = vec2;
v@_mcu_max_curv_dir = vec1;
f@_mcu_curvature = @_mcu_mean_curv;"	)

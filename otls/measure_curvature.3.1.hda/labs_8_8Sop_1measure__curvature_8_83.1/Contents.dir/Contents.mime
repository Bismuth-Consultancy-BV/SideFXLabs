--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/pointwrangle11.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	_mcu_unshared	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"// approximate attributes on boundary points

int neighbours[] = neighbours(0, @ptnum);
int num = len(neighbours);

float mean_curv = 0;
float gauss_curv = 0;
float max_curv = 0;
float min_curv = 0;
vector max_curv_dir = 0;
vector min_curv_dir = 0;
int count = 0;

foreach (int neighbour; neighbours)
{
    if (!inpointgroup(0, \"_mcu_unshared\", neighbour))
    {
        mean_curv += point(0, \"_mcu_mean_curv\", neighbour);
        gauss_curv += point(0, \"_mcu_gauss_curv\", neighbour);
        max_curv += point(0, \"_mcu_max_curv\", neighbour);
        min_curv += point(0, \"_mcu_min_curv\", neighbour);
        vector vec = point(0, \"_mcu_min_curv_dir\", neighbour);
        if (dot(vec, min_curv_dir) < 0)
            vec *= -1;
        min_curv_dir += vec;
        vec = point(0, \"_mcu_max_curv_dir\", neighbour);
        if (dot(vec, max_curv_dir) < 0)
            vec *= -1;
        max_curv_dir += vec;
        count += 1;
    }
}

f@_mcu_mean_curv = mean_curv / count;
f@_mcu_gauss_curv = gauss_curv / count;
f@_mcu_max_curv = max_curv / count;
f@_mcu_min_curv = min_curv / count;
v@_mcu_min_curv_dir = normalize(min_curv_dir);
v@_mcu_max_curv_dir = normalize(max_curv_dir);
f@_mcu_curvature = f@_mcu_mean_curv;"	)

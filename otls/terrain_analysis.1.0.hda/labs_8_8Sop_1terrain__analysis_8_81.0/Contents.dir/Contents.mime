snippet	[ 0	locks=0 ]	(	"// Declaration of Variables
float radius = chf(\"radius\");
float sides = chf(\"sides\");
float arc_start = radians(chf(\"arcstart\"));
float arc_end = radians(chf(\"arcend\"));
float pi = 3.14159265359;

float sides_counter = @ptnum % (sides + 1.0);

// Shape Generation
float angle = (((arc_start - arc_end)) / sides * sides_counter) - arc_start;
        
// Position
@P = set(cos(angle) * radius, 0.0, sin(angle) * radius);"	)

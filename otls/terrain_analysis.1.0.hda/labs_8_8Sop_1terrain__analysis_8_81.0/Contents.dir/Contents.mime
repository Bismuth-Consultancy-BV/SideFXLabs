--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/calculate_curvature.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int sample_count = npoints(2);
float ray_length = chf(\"ray_length\");
float ray_offset = chf(\"ray_offset\");
float exp = chf(\"exp\");

// Abbreviations
// h - horizontal, v - vertical, curv - curvature, dir - direction
// ex. h_curv_convex stands for horizontal_curvature_convex

float h_curv = 0.0;
float h_curv_convex = 0.0;
float h_curv_concave = 0.0;

// float v_curv = 0.0;
// float v_curv_convex = 0.0;
// float v_curv_concave = 0.0;

float h_weight_convex = 0.0;
float h_weight_concave = 0.0;

// float v_weight_convex = 0.0;
// float v_weight_concave = 0.0;

for (int pt = 0; pt < sample_count; pt++)
{
    vector h_dir = point(2, \"P\", pt);
    vector4 h_q = dihedral({0.0, 0.0, 1.0}, v@_tea_normal);
    h_dir = qrotate(h_q, h_dir);
    // vector v_dir = point(3, \"P\", pt);
    // vector4 v_q = dihedral({0, 0, 1}, v@N);
    // v_dir = qrotate(v_q, v_dir);

    float cos_theta = max(0.0, dot(h_dir, v@_tea_normal));
    float sin_theta = sqrt(1.0 - cos_theta * cos_theta);
    vector hit_P, hit_uvw;

    // if (chi(\"debug\"))
    // {
    //     addprim(0, \"polyline\", @ptnum, addpoint(0, @P + h_dir * ray_length * chf(\"debug_scale\")));
    // }

    // Horizontal Curvature
    if (intersect(1, @P + h_dir * ray_offset, h_dir * ray_length, hit_P, hit_uvw) != -1)
    {
        h_curv_convex += pow(distance(v@_tea_normal, hit_P) / ray_length, exp) * cos_theta;
    }

    else
    {
        h_curv_convex += cos_theta;
    }

    h_weight_convex += sin_theta;
    h_curv_convex = h_curv_convex / h_weight_convex;

    if (intersect(1, @P - h_dir * ray_offset, h_dir * ray_length * -1.0, hit_P, hit_uvw) != -1)
    {
        h_curv_concave += pow(distance(v@_tea_normal, hit_P) / ray_length, exp) * cos_theta;
    }

    else
    {
        h_curv_concave += cos_theta;
    }

    h_weight_concave += sin_theta;
    h_curv_concave = h_curv_concave / h_weight_concave;

    // // Vertical Curvature
    // if (intersect(1, @P + v_dir * ray_offset, v_dir * ray_length, hit_P, hit_uvw) != -1)
    // {
    //     v_curv_convex += pow(distance(v@_rta_normal, hit_P) / ray_length, exp) * cos_theta;
    // }

    // else
    // {
    //     v_curv_convex += cos_theta;
    // }

    // v_weight_convex += sin_theta;
    // v_curv_convex = v_curv_convex / v_weight_convex;

    // if (intersect(1, @P - v_dir * ray_offset, v_dir * ray_length * -1, hit_P, hit_uvw) != -1)
    // {
    //     v_curv_concave += pow(distance(v@_rta_normal, hit_P) / ray_length, exp) * cos_theta;
    // }

    // else
    // {
    //     v_curv_concave += cos_theta;
    // }

    // v_weight_concave += sin_theta;
    // v_curv_concave = v_curv_concave / v_weight_concave;
}

h_curv = max(h_curv_convex, h_curv_concave);
// v_curv = max(v_curv_convex, v_curv_concave);

f@_rta_horizontal_curvature = h_curv;
// f@_rta_vertical_curvature = v_curv;"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
vex_inplace	[ 0	locks=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0 ]	(	""	)
vex_precision	[ 0	locks=0 ]	(	auto	)
folder1	[ 0	locks=0 ]	(	0	0	)
ray_length	[ 0	locks=0 ]	(	[ ray_length	0 ] 	)
ray_offset	[ 0	locks=0 ]	(	[ ray_offset	0 ] 	)
exp	[ 0	locks=0 ]	(	[ exp	0 ] 	)
}


vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int rows_tile_original = chi(\"rows_original\");
int rows_tile_padded = chi(\"rows_padded\");

int padding = (rows_tile_padded - rows_tile_original) / 2;

int temp_row = @ptnum % rows_tile_padded - padding;
int temp_column = floor(@ptnum / rows_tile_padded) - padding;

if (temp_row < 0 || temp_column < 0 || temp_row >= rows_tile_original || temp_column >= rows_tile_original)
{
    i@_tea_index = -1;
}
else
{
    i@_tea_index = temp_row + temp_column * rows_tile_original;
}
"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)

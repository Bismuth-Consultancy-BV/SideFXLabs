def createNext(node):

    parent = node.parent()
    connections = node.outputs()
    
    createNext = parent.createNode('labs::biome_curve_setup')
    createNext.setInput(1, node, 0)
    parent.layoutChildren([node, createNext])

    if connections:
        for connect in connections:
            connect.setInput(1, createNext, 0)
            parent.layoutChildren([node, createNext])
            
        
    
    
    
    
